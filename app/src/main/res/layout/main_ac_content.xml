<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		app:layout_behavior="@string/appbar_scrolling_view_behavior"
		tools:showIn="@layout/main_ac_app_bar"
		tools:context="com.thecoppermind.MainActivity"
		tools:layout_editor_absoluteY="81dp"
		tools:layout_editor_absoluteX="0dp">

	<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">

		<Button
				android:id="@+id/main_ac_content_btn_holstep"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Holstep"/>

		<Button
				android:id="@+id/main_ac_content_btn_skaa"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Skaa rebellion"/>

		<Button
				android:id="@+id/main_ac_content_btn_vin"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Vin"/>

		<Button
				android:id="@+id/main_ac_content_btn_elend"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Elend Venture"/>
		<Button
				android:id="@+id/main_ac_content_btn_rashek"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Rashek"/>
	</LinearLayout>
</android.support.constraint.ConstraintLayout>
